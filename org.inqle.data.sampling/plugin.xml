<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point 
   		id="org.inqle.data.sampling.rap.ISamplerFactory" 
   		name="Sampler Factory" 
   		schema="schema/org.inqle.data.sampling.rap.ISamplerFactory.exsd"/>
   <extension
         point="org.inqle.ui.rap.IPartType">
			<part-type
           class="org.inqle.data.sampling.rap.Samplers"
           weight="1000">
			</part-type>
   </extension>
   <extension
         point="org.inqle.data.sampling.rap.ISamplerFactory">
      <sampler-factory
      		 class="org.inqle.data.sampling.rap.SimpleSparqlSamplerFactory"
           weight="1000"
           name="SimpleSparqlSampler"
           description="From 1 or more datasets, this Sampler selects 2-3 RDF predicates as attributes.  It then attempts to assemble a set of data which has these attributes in common.">
      </sampler-factory>
   </extension>

</plugin>
