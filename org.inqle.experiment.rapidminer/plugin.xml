<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point 
   		id="org.inqle.experiment.rapidminer.IRapidMinerExperiment" 
   		name="RapidMiner Experiment" 
   		schema="schema/org.inqle.experiment.rapidminer.IRapidMinerExperiment.exsd"/>
   <extension
         point="org.inqle.ui.rap.IPartType">
		<part-type
           class="org.inqle.experiment.rapidminer.RapidMinerExperimentParts"
           weight="3000"
           access="admin">
		</part-type>
		<part-type
           class="org.inqle.experiment.rapidminer.LearningCycleParts"
           weight="4000"
           access="admin">
		</part-type>
   </extension>
    <extension
         point="org.inqle.data.datamodels">
      	<datamodel
           id="org.inqle.datamodels.learningCycles"
           weight="7000"
           description="contains customized Learning Cycles"
					 cacheInMemory="false"
					 targetDatabase="org.inqle.data.databases.cache">
		</datamodel>
		<datamodel
           id="org.inqle.datamodels.experiments"
           weight="8000"
           description="contains experiment results"
					 cacheInMemory="false"
					 targetDatabase="org.inqle.data.databases.cache">
			</datamodel>
	</extension>
   <extension
         point="org.inqle.experiment.rapidminer.IRapidMinerExperiment">
      <!--rapidminer-experiment
      		 id="org.inqle.experiment.basic.SimpleRegressionNeuralNet"
      		 file="/org/inqle/experiment/rapidminer/xml/SimpleRegressionNeuralNet.xml"
      		 name="Simple Regression Using RapidMiner's Neural Net Learner"
      		 description="A simple regression experiment"
      		 type="regression"
           weight="1000">
      </rapidminer-experiment-->
      <!--rapidminer-experiment
      		 id="org.inqle.experiment.basic.SimpleRegressionM5P"
      		 file="/org/inqle/experiment/rapidminer/xml/SimpleRegressionM5P.xml"
      		 name="Simple Regression Using Weka's M5 Prime Decision tree learner"
      		 description="A simple regression experiment"
      		 type="regression"
           weight="1000">
      </rapidminer-experiment-->
      <rapidminer-experiment
      		 id="org.inqle.experiment.basic.SimpleClassificationDecisionTree"
      		 file="/org/inqle/experiment/rapidminer/xml/SimpleClassificationDecisionTree.xml"
      		 name="Decision Tree Classification"
      		 description="Simple Classification Using RapidMiner's Decision Tree Learner"
      		 type="classification"
           weight="1000">
      </rapidminer-experiment>
      <rapidminer-experiment
      		 id="org.inqle.experiment.basic.SimpleClassificationNearestNeighbor"
      		 file="/org/inqle/experiment/rapidminer/xml/SimpleClassificationNearestNeighbor.xml"
      		 name="Nearest Neighbor Classification"
      		 description="Simple Classification Using RapidMiner's Nearest Neighbor Learner"
      		 type="classification"
           weight="1000">
      </rapidminer-experiment>
      <rapidminer-experiment
      		 id="org.inqle.experiment.basic.SimpleRegressionNearestNeighbor"
      		 file="/org/inqle/experiment/rapidminer/xml/SimpleRegressionNearestNeighbor.xml"
      		 name="Nearest Neighbor Regression"
      		 description="Simple Regression Using RapidMiner's Nearest Neighbor Learner"
      		 type="regression"
           weight="1000">
      </rapidminer-experiment>
   </extension>

	<extension
         point="org.inqle.agent.rap.IAgentFactory">
		<agent
           class="org.inqle.experiment.rapidminer.agent.ExperimenterAgentFactory"
           name="Experimenter Agent"
           description="This agent runs learning cycles, generating experiment results"
           weight="1000"
           access="admin"/>
  </extension>
  
  <extension
         point="org.eclipse.ui.views">
      <!--view
            name="Experiments"
            icon="icons/microscope.jpeg"
            class="org.inqle.experiment.rapidminer.ExperimentsView"
            id="org.inqle.experiment.rapidminer.experimentsView">
      </view-->
      
      <view
            name="Experiments"
            icon="icons/microscope.jpeg"
            class="org.inqle.experiment.rapidminer.ExperimentsView"
            id="org.inqle.experiment.rapidminer.experimentsView">
      </view>
  </extension>
</plugin>
