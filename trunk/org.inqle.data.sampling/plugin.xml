<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point 
   		id="org.inqle.data.sampling.rap.ISamplerFactory" 
   		name="Sampler Factory" 
   		schema="schema/org.inqle.data.sampling.rap.ISamplerFactory.exsd"/>
   <extension
         point="org.inqle.ui.rap.IPartType">
			<part-type
           class="org.inqle.data.sampling.rap.Samplers"
           weight="1000">
			</part-type>
   </extension>
   <extension
         point="org.inqle.data.datamodels">
      <datamodel
           id="org.inqle.datamodels.samplers"
           weight="5000"
           description="contains samplers.">
					 cacheInMemory="false"
			</datamodel>
	</extension>
  <extension
         point="org.inqle.data.sampling.rap.ISamplerFactory">
      <sampler-factory
      		 class="org.inqle.data.sampling.rap.SimpleSubjectSparqlSamplerFactory"
           weight="1000"
           name="SimpleSubjectSparqlSampler"
           description="From 1 or more datamodels, this Sampler selects a subject class, then 2-3 Arcs as columns/attributes.  It then attempts to assemble the corresponding data into an ArcTable.">
      </sampler-factory>
   </extension>

</plugin>
