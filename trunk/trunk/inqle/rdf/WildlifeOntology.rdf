    <vs:term_status>testing</vs:term_status>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="distributionMap">
    <rdfs:comment xml:lang="en">associates a habitat, ecozone, or taxon rank with a map depicting its distribution or location</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="TaxonRank"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="Habitat"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="Ecozone"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>
              </rdf:Description>
            </rdf:rest>
          </rdf:Description>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:label xml:lang="en">distribution map</rdfs:label>
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Image"/>
    <vs:term_status>unstable</vs:term_status>
  </owl:ObjectProperty>
