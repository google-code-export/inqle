    <vs:term_status>testing</vs:term_status>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="ecozone">
    <rdfs:comment xml:lang="en">indicates that a habitat or a taxon rank can be found within an ecozone</rdfs:comment>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="TaxonRank"/>
            <rdf:rest>
