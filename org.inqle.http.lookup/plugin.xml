<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

	<extension point="org.eclipse.equinox.http.registry.servlets">
		<servlet
			alias="/lookup"
			class="org.inqle.http.lookup.LookupServlet"
			httpcontextId="org.eclipse.rap.httpcontext"
			load-on-startup="true">
		</servlet>
		
		<servlet
			alias="/export"
			class="org.inqle.http.lookup.ExportServlet"
			httpcontextId="org.eclipse.rap.httpcontext"
			load-on-startup="true">
		</servlet>
		
		<servlet
			alias="/datamodel"
			class="org.inqle.http.lookup.PublishDatamodelServlet"
			httpcontextId="org.eclipse.rap.httpcontext"
			load-on-startup="true">
		</servlet>
	</extension>
</plugin>
